import java.util.Scanner;

public class Main{
    public static void main(String [] args)
    {
        int index=1;
        while(index<=3)
        {
            System.out.println("Hello world");
            index++;
        }
        
        Scanner sc=new Scanner(System.in);
        do{
            System.out.println("welcome Jasleen");
            System.out.println("your rollno pls:");
            index=sc.nextInt();
        }while(index!=2003212);
        
        
        switch(index)
        {
            case 1:
                System.out.println("one attempt pass");
                break;
            case 5:
                System.out.println("fourth attempt pass");
                break;
            default:
                System.out.println("you failed the test");
                break;
        }
    }
}
